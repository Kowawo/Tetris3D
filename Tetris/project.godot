[application]

config/name="Tetris"
run/main_scene="res://Szenen/SceneSwitcher.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://Scripts/Globals.gd"
MusicController="*res://Szenen/MusicController.tscn"
Server="*res://Scripts/Multiplayer/Server.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=1440
window/size/height=810
window/size/resizable=false
mouse_cursor/custom_image="res://Image/MouseCursor.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Todo_Manager/plugin.cfg" )

[global]

verbose=true

